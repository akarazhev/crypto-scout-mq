{
  "rabbit_version": "4.1.4",
  "rabbitmq_version": "4.1.4",
  "product_name": "RabbitMQ",
  "product_version": "4.1.4",
  "users": [],
  "vhosts": [
    {
      "name": "/",
      "description": "Default virtual host",
      "tags": [],
      "metadata": {
        "description": "Default virtual host",
        "tags": []
      }
    }
  ],
  "permissions": [],
  "topic_permissions": [],
  "parameters": [],
  "global_parameters": [],
  "policies": [
    {
      "vhost": "/",
      "name": "stream-retention",
      "pattern": ".*-stream$",
      "definition": {
        "max-length-bytes": 2000000000,
        "max-age": "7D",
        "stream-max-segment-size-bytes": 100000000
      },
      "priority": 0,
      "apply-to": "queues"
    }
  ],
  "queues": [
    {
      "name": "crypto-bybit-stream",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length-bytes": 2000000000,
        "x-max-age": "7D",
        "x-queue-type": "stream",
        "x-stream-max-segment-size-bytes": 100000000
      }
    },
    {
      "name": "crypto-bybit-ta-stream",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length-bytes": 2000000000,
        "x-max-age": "7D",
        "x-queue-type": "stream",
        "x-stream-max-segment-size-bytes": 100000000
      }
    },
    {
      "name": "metrics-bybit-stream",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length-bytes": 2000000000,
        "x-max-age": "7D",
        "x-queue-type": "stream",
        "x-stream-max-segment-size-bytes": 100000000
      }
    },
    {
      "name": "collector-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length": 2500,
        "x-message-ttl": 21600000,
        "x-queue-mode": "lazy",
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "crypto-scout-chatbot-queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length": 2500,
        "x-message-ttl": 21600000,
        "x-queue-mode": "lazy",
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "metrics-cmc-stream",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-length-bytes": 2000000000,
        "x-max-age": "7D",
        "x-queue-type": "stream",
        "x-stream-max-segment-size-bytes": 100000000
      }
    }
  ],
  "exchanges": [
    {
      "name": "metrics-exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "crypto-exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "collector-exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "collector-exchange",
      "vhost": "/",
      "destination": "collector-queue",
      "destination_type": "queue",
      "routing_key": "crypto-scout-collector",
      "arguments": {}
    },
    {
      "source": "collector-exchange",
      "vhost": "/",
      "destination": "crypto-scout-chatbot-queue",
      "destination_type": "queue",
      "routing_key": "crypto-scout-chatbot",
      "arguments": {}
    },
    {
      "source": "crypto-exchange",
      "vhost": "/",
      "destination": "crypto-bybit-stream",
      "destination_type": "queue",
      "routing_key": "crypto-bybit",
      "arguments": {}
    },
    {
      "source": "crypto-exchange",
      "vhost": "/",
      "destination": "crypto-bybit-ta-stream",
      "destination_type": "queue",
      "routing_key": "crypto-bybit-ta",
      "arguments": {}
    },
    {
      "source": "metrics-exchange",
      "vhost": "/",
      "destination": "metrics-bybit-stream",
      "destination_type": "queue",
      "routing_key": "metrics-bybit",
      "arguments": {}
    },
    {
      "source": "metrics-exchange",
      "vhost": "/",
      "destination": "metrics-cmc-stream",
      "destination_type": "queue",
      "routing_key": "metrics-cmc",
      "arguments": {}
    }
  ]
}